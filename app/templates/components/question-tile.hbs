{{#link-to 'question' question.id}}
<div class="col-md-4 question-tile">
  <div class="row">
    <h3>{{question.content}}</h3>
    <h5>from {{question.name}}</h5>
    <div class="">
      <img class="question-tile-image" src="{{question.image}}" alt="image" />
    </div>
  </div>
</div>
{{/link-to}}
